jQuery(document).ready(function ($) {

    if($("form.reset-password").length > 0) {

        $("#main-menu").hide();        
        $("span.privacypolicy").hide();        
        $("ul.tabs.primary").hide();
        $("dd.profile").hide();       
            
    }       

}); what does this code means

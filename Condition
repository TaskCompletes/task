const endPoint = documentElement.dataset.endpoint;
const splitedEndpoint = endPoint.split('/');
let reset = false;

if (splitedEndpoint[4].endsWith('v-0')) {
    const fileName = file.name;
    const some = fileName.split('.');
    const resolveExtension = documentElement.attributes.resolve_extension.value;
    const [type1, type2] = resolveExtension.split(' ');
    const [splitBySlashType1, , splitBySlashType2] = [type1, type2].map(type => type.split('/'));
    reset = true;

    const some2 = `/${splitedEndpoint[1]}/${splitedEndpoint[2]}/${splitedEndpoint[3]}/${splitBySlashType1[2]}-0`;
    const some3 = `/${splitedEndpoint[1]}/${splitedEndpoint[2]}/${splitedEndpoint[3]}/${splitBySlashType2[2]}-0`;

    this.endpoint = some[1] === splitBySlashType1[0] ? some2 : (some[1] === splitBySlashType2[0] ? some3 : undefined);

    console.log(this.endpoint);
    console.log(this.endpoint ? ' from first/second if' : undefined);
}

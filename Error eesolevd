public function addAjaxconatainer() : self {
    $this->form['notifications']['info_box'] = [
      '#type' => 'container',
      '#attributes' => [
        'id' => 'file-candy',
      ],
    ];
    return $this;
   }
   public function addRadioButton(): Self{
 
    $this->form['country'] = [
      '#type' => 'radios',
      '#title' => $this->t('Important information'),
      '#options' => [
        'no' => 'No',
        'yes' => 'Yes',
      ],
      '#default_value' => 'no',
    ];
    return $this;
   }
   public function addTextFieldOne(): self{
    $this->form['notifications']['info_box']['textField_one'] = [
      '#type' => 'textfield',
      '#size' => 1,
      '#maxlength' => 1,
      '#states' => [
        'visible' => [
          ":input[name = 'country']" => ['value' => 'yes']
        ]
      ],
      '#ajax' => [
        'callback' => '::updateAuthMessage',
        'progress' => array('type' => 'none'),
        'wrapper' => 'file-candy',
        'event' => 'change',
      ],
    ];
    return $this;
   }
   public function addAuthMessage(): self {
    $this->form['notifications']['info_box']['auth_message'] = [
      '#markup' => 'hiii this important'
    ];
    return $this;
   }

function displaysomething(hasLowercase,hasUppercase,hasNumbers,password){
  let good = "<span class = 'good'> good </span>";

  if (password.length >= 7) {
    
    if(criteriaMessages.tooShort.includes(good) == false)
    {
         criteriaMessages.tooShort = criteriaMessages.tooShort.concat(good);
         
    }
    else
    {
         criteriaMessages.tooShort;
    }
    
  }
  if (hasLowercase) {
    
    if(criteriaMessages.addLowerCase.includes(good) == false)
    {
         criteriaMessages.addLowerCase = criteriaMessages.addLowerCase.concat(good);
    }
    else
    {   
         criteriaMessages.addLowerCase;
    }
    
  }
  if (hasUppercase) {
    if(criteriaMessages.addUpperCase.includes(good) == false)
    {
         criteriaMessages.addUpperCase = criteriaMessages.addUpperCase.concat(good);
         
    }
    else
    {
         criteriaMessages.addUpperCase;
    }
    
  }
  if (hasNumbers) {
     
     if(criteriaMessages.addNumbers.includes(good) ==  false)
     {
         criteriaMessages.addNumbers = criteriaMessages.addNumbers.concat(good);
         
     }
     else
     {
         criteriaMessages.addNumbers;
     } 
  }
  if (password.length < 7) {
  
    if(criteriaMessages.tooShort.includes(good))
    {
         criteriaMessages.tooShort = criteriaMessages.tooShort.replace(good,"");
    }
    else
    {
       criteriaMessages.tooShort;
    }
  }

  if (!hasLowercase) {
  
    if(criteriaMessages.addLowerCase.includes(good))
    {
       criteriaMessages.addLowerCase.replace(good,"");
       
    }
    else
    {
       criteriaMessages.addLowerCase;
    }
  }
  if (!hasUppercase) {
   
    if(criteriaMessages.addUpperCase.includes(good))
    {
      criteriaMessages.addUpperCase.replace(good,"");
    }
    else
    {
     criteriaMessages.addUpperCase;
     
    }
  }
  if (!hasNumbers) { 
    if(criteriaMessages.addNumbers.includes(good))
    {
      criteriaMessages.addNumbers.replace(good,"");
    }
    else
    {
     criteriaMessages.addNumbers;
    }
  }
  displaySuggestions(criteriaMessages);
